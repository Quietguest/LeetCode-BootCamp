class Solution(object):
    def twoSum(self, numbers, target):
        """
        :type numbers: List[int]
        :type target: int
        :rtype: List[int]
        """
        ans = []
        left = 0
        right = len(numbers) - 1
        while left < right:
            s = numbers[left] + numbers[right]
            if s == target:
                ans = left + 1, right + 1
                left += 1
                right -= 1
            elif s < target:
                left += 1
            else:
                right -= 1
                
        return ans
